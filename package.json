{
  "name": "insurancecompany",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "start:prod": "node start-production.js",
    "dev": "nodemon --exec \"ts-node --esm --experimental-specifier-resolution=node\" index.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:prod": "NODE_ENV=production tsc",
    "dev:ts": "ts-node --esm index.ts",
    "dev:ts:watch": "nodemon --exec \"ts-node --esm --experimental-specifier-resolution=node\" index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=controllers",
    "test:all": "node tests/run-tests.js",
    "test:email": "node test-email.js",
    "test:custom-email": "node test-custom-email.js",
    "test:inbox": "node test-inbox.js",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "db:indexes": "ts-node --esm src/scripts/addDatabaseIndexes.ts",
    "cache:stats": "curl -X GET http://localhost:3000/api/v1/cache/stats",
    "cache:clear": "curl -X POST http://localhost:3000/api/v1/cache/clear",
    "test:v1-v3": "node test-v1-v3-endpoints.js",
    "test:cors": "node test-cors-fix.js",
    "test:cors:comprehensive": "node test-cors-comprehensive.js",
    "test:auth": "node test-authentication.js",
    "setup:user": "node setup-test-user.js",
    "fix:users": "node fix-user-status.js",
    "monitor:start": "ts-node --esm src/monitoring/performanceMonitor.ts",
    "load:test": "artillery run tests/load/artillery.yml",
    "load:test:report": "artillery run --output reports/load-test-report.json tests/load/artillery.yml",
    "db:optimize": "ts-node --esm src/scripts/databaseOptimizer.ts",
    "cache:warm": "ts-node --esm src/scripts/cacheWarmer.ts",
    "api:version": "ts-node --esm src/scripts/apiVersionManager.ts",
    "prod:setup": "npm run build:prod && npm run start:prod",
    "prod:deploy": "npm run build:prod",
    "health:check": "curl -f http://localhost:3000/api/v1/health || exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0",
    "isomorphic-dompurify": "^2.9.3",
    "joi": "^17.13.3",
    "jsdom": "^24.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.12.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "node-imap": "^0.9.6",
    "nodemailer": "^6.10.0",
    "prom-client": "^15.1.3",
    "redis": "^5.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.0.0",
    "babel-jest": "^30.0.4",
    "jest": "^30.0.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
